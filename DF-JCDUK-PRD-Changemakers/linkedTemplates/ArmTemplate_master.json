{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DF-JCDUK-PRD-Changemakers"
		},
		"AzureSqlDatabase_Changemakers_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_Changemakers'"
		},
		"GACall_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'GACall'"
		},
		"GoogleAnalytics_GetFiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'GoogleAnalytics_GetFiles'"
		},
		"GACall_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://getoauthkey-uk.azurewebsites.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DF-JCDUK-PRD-Changemakers_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_Changemakers_connectionString": {
						"value": "[parameters('AzureSqlDatabase_Changemakers_connectionString')]"
					},
					"GACall_functionKey": {
						"value": "[parameters('GACall_functionKey')]"
					},
					"GoogleAnalytics_GetFiles_connectionString": {
						"value": "[parameters('GoogleAnalytics_GetFiles_connectionString')]"
					},
					"GACall_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('GACall_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}